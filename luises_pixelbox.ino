// We need to put this array into EEPROM (PROGMEM modifier) because it
// references data in EEPROM - and it's just too messy to put an entire
// data structure in there (the read and write library functions are
// really rudimentary) so the trade-off is that we're now managing two
// separate and decoupled structures, one for animation data and another
// for animation control (the latter is placed in RAM), and it's the
// programmer's responsibility to make sure each record in the two
// structures correspond.

static const unsigned char* const sequence[] PROGMEM = { 
  Mario1, Mario2, Mario3, 0,  // 0 pointer means end of sequence
  Mushroom1, Mushroom2, 0,
  BB1, BB2, BB3, BB4, 0,
  Ghostred1, Ghostred2, Ghostred1, Ghostred2, 0,
  Pacman3, Pacman1, Pacman2, Pacman1, 0,
  Ghostblue1, Ghostblue2, 
  Ghostwhite1, Ghostwhite2, 0,
  Frogger1, Frogger2, Frogger3, 0,
  0 // empty list means end of set
};

// fiddle with this until the matrix and the uno draws less than 2Amps
#define MAX_BRIGHTNESS 45

void setup() {
  matrix.begin();
  matrix.setBrightness(MAX_BRIGHTNESS);
// Serial.begin(9600);
}

int billede = 0;
int last_sequence = billede;
int loops = 3;
int loop_cnt = loops;

void loop() 
{
  const unsigned char* data = (const unsigned char*) pgm_read_word(&(sequence[billede]));
  
  // work out which image we're going to display
  
  if (!data) {	// last image in a sequence?
    if (!--loop_cnt) {   // last sequence?
      loop_cnt = loops;
      last_sequence = ++billede;
      if ((const unsigned char*) pgm_read_word(&(sequence[billede])) == 0)
        billede = last_sequence = 0;  // start all over
      return;
    }
 
    billede = last_sequence;
    return;
  }
  else billede++;

  int i, j;
  for (i = 0; i < 256; i++) {
    
    int r = pgm_read_byte_near(data + i*3 + 0);
    int g = pgm_read_byte_near(data + i*3 + 1);
    int b = pgm_read_byte_near(data + i*3 + 2);

    // transparent colour magic value
    if (r == 255 && g == 0 && b == 255)
      r = b = g = 0;  // just set it to off 

    if (i & 0x10) // hvis vi er paa en ulige linje
      j = (i & 0xF0) + 0x0F - (i & 0x0F); // fra højre
    else
      j = i;  // ellers fra venstre

    matrix.setPixelColor(j, r, g, b);

  }

  delay(200);
  
  matrix.show();
}
